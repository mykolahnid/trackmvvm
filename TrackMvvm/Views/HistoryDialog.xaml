<Window x:Class="TrackMvvm.Views.HistoryDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TrackMvvm.Views"
        xmlns:xcdg="http://schemas.xceed.com/wpf/xaml/datagrid"
        mc:Ignorable="d"
        Title="HistoryDialog" Height="450" Width="800">

    <Window.DataContext>
        <Binding Path="HistoryViewModel" Source="{StaticResource Locator}"/>
    </Window.DataContext>
    <Grid>
        <DataGrid ItemsSource="{Binding History}" AutoGenerateColumns="True" AutoGeneratedColumns="DataGrid_OnAutoGeneratedColumns"></DataGrid>

    </Grid>
</Window>
